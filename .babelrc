{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [
          ".js",
          ".ios.js",
          ".android.js",
          ".ts",
          ".ios.ts",
          ".android.ts"
        ],
        "alias": {
          "@/assets": "./src/assets",
          "@/components": "./src/components",
          "@/navigations": "./src/navigations",
          "@/screens": "./src/screens",
          "@/styles": "./src/styles",
          "@/utils": "./src/utils",
          "@/context": "./src/context"
        }
      }
    ]
  ]
}
